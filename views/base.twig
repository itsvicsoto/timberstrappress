{% block html_head_container %}
    {% include 'html-header.twig' %}
    {% block head %}
    {% endblock %}
    </head>
{% endblock %}
<body class="{{ body_class }}" data-template="base.twig">

<div class="header">
  <div class="container">

    {% block header %}

    <!-- Fixed navbar -->
    <nav class="navbar {{ page.headerClass }} navbar-fixed-top" {% if page.headerClass != 'navbar-theme-white' %}data-ui="navbarHeader"{% endif %}>
      <div class="container">
        <div class="row">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ site.url }}" title="{{ site.name }}">
              {% if page.headerClass != 'navbar-theme-white' %}
              <img src="{{ site.theme.link }}/img/raw/formgroup_logo.svg" width="100" alt="" />
              {% endif %}
              {% if page.headerClass == 'navbar-theme-white' %}
              <img src="{{ site.theme.link }}/img/raw/formgroup_logo_colored.svg" width="100" alt="" />
              {% endif %}
            </a>
          </div>
          <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
              <li class="active"><a href="/contact" class="navbar__call-link">CALL US AT (02) 9529 . 6333</a></li>
            </ul>
            <ul class="nav navbar-nav navbar-right">
              {% for item in menu.get_items %}
                  <li>
                    <a href="{{ item.get_link }}" class="{{ item.classes | join(' ') }} {% if item.current %}active{% endif %} ">{{ item.title }}</a>
                  </li>
              {% endfor %}
            </ul>
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </nav>

    {% endblock %}

  </div>
</div>

{#START main contianer#}
<div class="container-fluid">
  <div class="row">
    {% block content %}
        Sorry, no content
    {% endblock %}
  </div>
</div>
{#/.main-content#}

{#START footer container#}
{% block footer %}
    {% include 'footer.twig' %}
    {{ function('wp_footer') }}
{% endblock %}
{#/.footer#}

</body>
</html>
